- if(resumeProfile.languages.length > 0) {
	//TODO: Add ability for each thing to have a note.
	- resumeProfile.languages.sort(function(a, b) {
	-	var dLevel = b.level - a.level;
	-	if(dLevel) return dLevel;
	-
	-	var dLevelDetailed = b.level_detailed - a.level_detailed;
	- 	return dLevelDetailed;
	- });

	- var columnSize = Math.ceil(resumeProfile.languages.length / 2);
	- var firstColumn = resumeProfile.languages.splice(0, columnSize);
	- var secondColumn = resumeProfile.languages;

	div(class="row")
		div(class="col-md-6")
			ul(class="no-bullets")
				each language in firstColumn
					li
						span(class="ability-title")= language.title
						span(class="ability-score")
							- var filled = new Array(language.level);
							each tmp in filled
								span(class="glyphicon glyphicon-star filled")

							- if(language.level < 5) {
							- var notFilled = new Array(5 - language.level);
							each tmp in notFilled
								span(class="glyphicon glyphicon-star")
							- }
						
					
		- if(columnSize >= 1) {
		div(class="col-md-6")
			ul(class="no-bullets")
				each language in secondColumn
					li
						span(class="ability-title")= language.title
						span(class="ability-score")
							- var filled = new Array(language.level);
							each tmp in filled
								span(class="glyphicon glyphicon-star filled")

							- if(language.level < 5) {
							- var notFilled = new Array(5 - language.level);
							each tmp in notFilled
								span(class="glyphicon glyphicon-star")
							- }
		- }

- } else {
div(class="alert alert-warning") No languages were found in this resume.
- }
