{
	"name" : "website",
	"dependencies": {
		"jquery": "~1.12.4",
		"bootstrap": "~3.3.5",
		"less": "~2.7.1",
		"normalize-css": "~5",
		"font-awesome" : "~4.6.3"
	},
	"exportsOverride": {
		"less": {
			"../js/": "dist/less.js"
		},

		"normalize-css": {
			"../css/": "normalize.css"
		},
		"jquery": {
			"../js/": "dist/jquery.*"
		},
